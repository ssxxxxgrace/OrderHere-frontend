{
  "name": "orderhere",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && npm run lint:all",
    "export": "next build && next export",
    "test": "jest -c jest.config.js --colors --coverage --passWithNoTests",
    "test:watch": "jest -c jest.config.js --colors --coverage --passWithNoTests --watch",
    "lint:eslint": "eslint --ext .jsx,.js",
    "lint:sass": "sass-lint -v",
    "lint:prettier": "prettier -c .prettierrc \"{pages,components,utils}/**/*.{js,jsx}\" --check",
    "lint:all": "npm run prettier && npm run lint:eslint && npm run lint:sass && npm run lint:prettier",
    "prepare": "husky install",
    "prettier": "prettier -c .prettierrc \"{pages,components,utils,store}/**/*.{js,jsx}\" --write"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.16",
    "@mui/lab": "^5.0.0-alpha.151",
    "@mui/material": "^5.14.16",
    "@mui/system": "^5.14.16",
    "axios": "^0.25.0",
    "cropperjs": "^1.5.12",
    "formik": "^2.2.9",
    "html-react-parser": "^1.4.8",
    "isomorphic-dompurify": "^0.18.0",
    "jest-mock": "^27.5.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "net": "^1.0.2",
    "next": "^14.0.1",
    "next-remove-imports": "^1.0.6",
    "nextjs-progressbar": "^0.0.14",
    "react": "^17.0.0 || ^18.0.0",
    "react-beautiful-dnd": "^13.1.0",
    "react-cropper": "^2.1.8",
    "react-dom": "^17.0.0 || ^18.0.0",
    "react-draggable": "^4.4.4",
    "react-dropzone": "^12.0.4",
    "react-hot-toast": "^2.2.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sass": "^1.49.7",
    "sharp": "^0.30.4",
    "styled-components": "^5.3.3",
    "swr": "^1.2.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@next/eslint-plugin-next": "^12.0.7",
    "@redux-devtools/extension": "^3.2.2",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "eslint": "^8.13.0",
    "eslint-config-next": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": ">=6",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^19.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.5.1",
    "sass-lint": "^1.13.1"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,json,css,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
